<?xml version="1.0"?>
<!--所有的长度以象素为单位，如果要转化为box2d的尺寸，需除以PTM_RATIO(32)-->
<!--每个myObj可以包含0到1个body和1个sprite-->
<!--对于没有body的obj，sprite的pos才起作用-->
<myObjList>
    <myObj name="groundBlock0" sceneNodeType="scene_rolling">
        <!--box2d body-->
        <body>
            <bodyType>staticBody</bodyType>
            <fixture>
                <shape>
                    <shapeType>polygon</shapeType>
                    <c>0 0</c>
                    <r>0</r>
                    <rxy>576 96</rxy>
                    <rot>0</rot>
                    <vertices></vertices>
                </shape>
                <friction>0.5</friction>
                <restitution>0</restitution>
                <density>0</density>
                <isSensor>0</isSensor>
                <filter>
                    <categoryBits>1</categoryBits><!--0x0001-->
                    <maskBits>65535</maskBits><!--0xFFFF-->
                    <groupIndex>0</groupIndex>
                </filter>
            </fixture>
            <pos>192 32</pos>
            <rot>0</rot>
            <gravityScale>1</gravityScale>
        </body>
        <!--cocos2d-x sprite-->
        <sprite>
            <useTexRect>0</useTexRect><!--相当于rect(0,0,texWidth,texHeight)-->
            <rect>0 0 1152 192</rect><!--当使useTexRect为false时才有效-->
            <tex>
                <texImage>data/global/tex/roadStone.png</texImage>
                <wrapS>repeat</wrapS>
                <wrapT>repeat</wrapT>
                <specifyTexCoord>0</specifyTexCoord>
                <quadTexCoord>
                    <tl>0 0</tl>
                    <tr>0 0</tr>
                    <bl>0 0</bl>
                    <br>0 0</br>
                </quadTexCoord>
            </tex>
            <zOrder>0</zOrder>
            <anchorPoint>0.5 0.5</anchorPoint>
            <pos>0 0</pos>
        </sprite>
    </myObj>
